	urldate = {2022-11-17},
	journal = {J. Dyn. Syst. Meas. Control},
	author = {Habibnejad Korayem, Amin and Hashemi, Ehsan and Khajepour, Amir and Fidan, Baris},
	month = may,
	year = {2022},
	pages = {051005},
}

