	keywords = {Accelerometers, Antenna measurements, GPS sideslip measurements, Global Positioning System, Global Positioning System (GPS)/Inertial Navigation System (INS) vehicle state estimation, Goniometers, Inertial navigation, Position measurement, Road vehicles, Sensor systems, State estimation, Tires, roll estimation, sideslip estimation},
